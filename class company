class Person:
    com="TCS"
    def __init__(self,name,age):
        self.name=name
        self.age=name
    def display(self):
        print("name:",self.name)
        print("age:",self.age)
        print("Company",Person.com)
s1=Person("RAM",40)
s=Person("SUMBIT",30)
s.display()
s1.display()
        
